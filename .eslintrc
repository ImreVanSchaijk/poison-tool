{
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-console": ["warn", { "allow": ["warn", "info", "error"] }],
      "react/no-did-update-set-state": 0,
      "import/prefer-default-export": 0
    },
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "babel-module": {
          "root": ["./src"]
        }
      }
    }
}
